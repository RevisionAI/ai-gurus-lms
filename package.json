{
  "name": "ai-gurus-lms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:both": "concurrently \"npm run dev\" \"cd vibe-code-lms && npm run dev\" --names \"AI-Fluency,Vibe-Code\" --prefix-colors \"blue,magenta\"",
    "build": "prisma generate && next build",
    "build:both": "npm run build && cd vibe-code-lms && npm run build",
    "start": "concurrently \"npm run dev\" \"cd vibe-code-lms && npm run dev\" --names \"AI-Fluency,Vibe-Code\" --prefix-colors \"blue,magenta\"",
    "start:prod": "next start -p 3000",
    "start:both": "concurrently \"npm run start:prod\" \"cd vibe-code-lms && npm run start\" --names \"AI-Fluency,Vibe-Code\" --prefix-colors \"blue,magenta\"",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:a11y": "playwright test accessibility.spec.ts keyboard-navigation.spec.ts",
    "test:a11y:ui": "playwright test accessibility.spec.ts keyboard-navigation.spec.ts --ui",
    "test:a11y:headed": "playwright test accessibility.spec.ts keyboard-navigation.spec.ts --headed",
    "test:a11y:chromium": "playwright test accessibility.spec.ts keyboard-navigation.spec.ts --project=chromium",
    "validate:migration": "npx tsx scripts/validate-migration.ts",
    "validate:performance": "npx tsx scripts/measure-performance.ts",
    "db:seed": "npx tsx scripts/seed-sample-data.ts",
    "r2:test": "npx tsx scripts/test-r2-upload.ts",
    "db:migrate-modules": "npx tsx prisma/scripts/migrate-to-modules.ts",
    "db:migrate-modules:rollback": "npx tsx prisma/scripts/migrate-to-modules.ts --rollback"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.939.0",
    "@aws-sdk/lib-storage": "^3.939.0",
    "@aws-sdk/s3-request-presigner": "^3.939.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.9.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-tabs": "^1.1.12",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.0",
    "@sentry/nextjs": "^10.27.0",
    "@tinymce/tinymce-react": "^6.2.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/dompurify": "^3.0.5",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/sanitize-html": "^2.16.0",
    "@types/three": "^0.181.0",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.6",
    "bcryptjs": "^3.0.2",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "framer-motion": "^12.23.24",
    "lenis": "^1.3.15",
    "lucide-react": "^0.514.0",
    "maath": "^0.10.8",
    "next": "15.3.3",
    "next-auth": "^4.24.11",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "prisma": "^6.9.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "recharts": "^3.5.0",
    "sanitize-html": "^2.17.0",
    "three": "^0.181.2",
    "tinymce": "^7.9.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-mock-extended": "^3.0.7",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.5",
    "typescript": "^5"
  }
}
